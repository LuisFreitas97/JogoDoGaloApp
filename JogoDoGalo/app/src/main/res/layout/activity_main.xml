<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.luis.jogodogalo.MainActivity"
    android:background="@drawable/tabuleiro">

    <Button
        android:id="@+id/b1"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="21dp"
        android:background="#000000"
        android:onClick="main"
        android:tag="1" />

    <Button
        android:id="@+id/b9"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_below="@+id/b5"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="19dp"
        android:layout_toEndOf="@+id/b5"
        android:layout_toRightOf="@+id/b5"
        android:background="#000000"
        android:onClick="main"
        android:tag="9"/>

    <Button
        android:id="@+id/b5"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="#000000"
        android:onClick="main"
        android:tag="5"/>

    <Button
        android:id="@+id/b7"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignLeft="@+id/b1"
        android:layout_alignStart="@+id/b1"
        android:layout_alignTop="@+id/b9"
        android:background="#000000"
        android:onClick="main"
        android:tag="7"/>

    <Button
        android:id="@+id/b3"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignBottom="@+id/b2"
        android:layout_alignLeft="@+id/b9"
        android:layout_alignStart="@+id/b9"
        android:background="#000000"
        android:onClick="main"
        android:tag="3"/>

    <Button
        android:id="@+id/b4"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignEnd="@+id/b7"
        android:layout_alignRight="@+id/b7"
        android:layout_centerVertical="true"
        android:background="#000000"
        android:onClick="main"
        android:tag="4"/>

    <Button
        android:id="@+id/b6"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignEnd="@+id/b3"
        android:layout_alignRight="@+id/b3"
        android:layout_centerVertical="true"
        android:background="#000000"
        android:onClick="main"
        android:tag="6"/>

    <Button
        android:id="@+id/b2"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignLeft="@+id/b5"
        android:layout_alignStart="@+id/b5"
        android:layout_alignTop="@+id/b1"
        android:background="#000000"
        android:onClick="main"
        android:tag="2"/>

    <Button
        android:id="@+id/b8"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_alignTop="@+id/b9"
        android:layout_toLeftOf="@+id/b9"
        android:layout_toStartOf="@+id/b9"
        android:background="#000000"
        android:onClick="main"
        android:tag="8" />
</RelativeLayout>
